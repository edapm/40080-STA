library(tidyverse)
library(car)
library(mctest)
library(olsrr)
rm(list=ls())
data <- read.csv("data.csv", header=T, sep=",")
data <- read.csv("Assessment/data.csv", header=T, sep=",")
# initial stepwise
mod <- lm(rainfall~., data = data)
mod_step <- step(mod, direction = "both")
# summary
summary(mod_step)
# assumptions
hist(mod_step$residuals)
ols_vif_tol(mod_step)
# new model (mc violated)
mod_step_2 <- update(mod_step, ~.-w)
# assumptions (model #2)
summary(mod_step_2)
hist(mod_step_2$residuals)
ols_vif_tol(mod_step_2)
# assumptions (model #2)
summary(mod_step_2)
View(mod_step_2)
# assumptions
hist(mod_step)
# assumptions
plot(mod_step)
install.packages(c("cli", "pbkrtest", "scales"))
plot(mod_step_2)
library(tidyverse)
library(car)
